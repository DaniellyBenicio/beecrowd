#include <stdio.h>
#include <math.h>//biblioteca de matem√°tica

int main() {
 
    double A, B, C, delta, x1, x2;
    
    scanf("%lf", &A);
    scanf("%lf", &B);
    scanf("%lf", &C);
   
    
    delta = ((B*B)-(4*A*C));
    // printf("%lf\n", delta);
   
    
    if(delta<0 || A==0){
    	printf("Impossivel calcular\n");
    }
    
    if(!(delta<0 || A==0)){
        
   		x1 = (-B + sqrt(delta)) /(2*A);
    	x2 = (-B - sqrt(delta)) /(2*A);
    
    	printf("R1 = %.5lf\n", x1);
    	printf("R2 = %.5lf\n", x2);
    }
    
    return 0;
}
